{
  "name": "复制助手",
  "version": "1.9",
  "description": "复制助手",
  "browser_action": {
    "default_icon": "logo.gif"
  },
  "background_page": "background.html",
  "permissions": ["<all_urls>","http://translate.google.cn/*","notifications","contextMenus","tabs"],
  "icons": {
	"16": "16.gif"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}